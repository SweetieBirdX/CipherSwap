# Server Configuration
PORT=3001
NODE_ENV=development

# API Keys (Required)
INCH_API_KEY=your_1inch_api_key_here
INFURA_KEY=your_infura_key_here
ALCHEMY_KEY=your_alchemy_key_here

# Blockchain Configuration
CHAIN_ID=1
PRIVATE_KEY=your_private_key_here

# RPC URLs
ETHEREUM_RPC_URL=https://mainnet.infura.io/v3/your_infura_key
ARBITRUM_RPC_URL=https://arb-mainnet.g.alchemy.com/v2/your_alchemy_key
BASE_RPC_URL=https://mainnet.base.org
ZKSYNC_RPC_URL=https://mainnet.era.zksync.io

# Chainlink Configuration
CHAINLINK_ORACLE_ADDRESS=your_chainlink_oracle_address_here

# Security
CORS_ORIGIN=http://localhost:5173
RATE_LIMIT_MAX=100

# Feature Flags
ENABLE_MEV_PROTECTION=true
ENABLE_FUSION=true
ENABLE_L2_SUPPORT=true
ENABLE_LIMIT_ORDERS=true
ENABLE_RESOLVER_BOT=false

# Frontend Configuration
VITE_API_URL=http://localhost:3001/api

# Optional API Keys
COINGECKO_API_KEY=your_coingecko_api_key_here
DEFILLAMA_API_KEY=your_defillama_api_key_here
THEGRAPH_API_KEY=your_thegraph_api_key_here

# Flashbots Configuration
FLASHBOTS_SIGNER_PRIVATE_KEY=your_flashbots_signer_private_key_here 